{
	"name": "Duolinguo Speech",
	"manifest_version": 3,
	"version": "3.0",
	"description": "Speech to text extension",
	"icons": {
		"16": "/icons/Icon16.png",
		"48": "/icons/Icon48.png",
		"128": "/icons/Icon128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.duolingo.com/*"
			],
			"js": [
				"/content/languages.js",
				"/content/index.js"
			],
			"css": [
				"/styles/style.css"
			],
			"all_frames": false
		}
	]
}